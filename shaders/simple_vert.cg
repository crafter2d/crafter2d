// Default vertex shader
// Jeroen Broekhuizen, 2005

void main(
	in float4 position : ATTR0,
	in float4 texCoord : ATTR8,
	out float4 positionO : POSITION,
	out float4 texCoord0O : TEXCOORD0,
	uniform float4x4 modelviewproj)
{
	positionO = mul(modelviewproj, position);
	texCoord0O = texCoord;
}